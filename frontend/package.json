{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "cypress": "cypress open",
    "cypress:run": "cypress run --component",
    "cypress:ci": "cypress:run",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:openapi": "npx openapi-typescript http://localhost:8000/openapi.json --output src/infraops.d.ts",
    "prettier": "prettier './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "prettier:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format-code": "npm run prettier:fix && npm run eslint:fix"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@headlessui/react": "^1.7.10",
    "@heroicons/react": "^2.0.15",
    "@hookform/error-message": "^2.0.1",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "date-fns": "^2.29.3",
    "graphql-request": "^5.1.0",
    "handlebars": "^4.7.7",
    "jotai": "^2.0.2",
    "query-string": "^7.1.3",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.2",
    "typescript": "^4.9.5",
    "use-query-params": "^2.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/ramda": "^0.28.23",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "cypress": "11.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "handlebars-jest": "^1.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "openapi-typescript": "^6.1.1",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {},
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|decode-uri-component|filter-obj|split-on-first|query-string)/)"
    ]
  }
}
